{
  "name": "WanderlyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "npx react-native-clean-project",
    "clean-files": "rm -rf yarn.lock && rm -rf Gemfile.lock && rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/Gemfile.lock  && rm -rf ios/Podfile.lock && rm -rf ios/build && rm -rf ios/.xcode.env.local && rm -rf android/Gemfile.lock",
    "setup": "yarn; yarn gem:install; yarn pod:install; yarn gradle;",
    "gem:install": "bundle install; cd ios; bundle install; cd ..; cd android; bundle install; cd ..;",
    "pod:install": "cd ios; pod install; cd ..;",
    "gradle": "cd android; ./gradlew clean; cd .."
  },
  "dependencies": {
    "@react-native-firebase/app": "^20.3.0",
    "react": "18.2.0",
    "react-native": "0.74.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
