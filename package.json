{
  "name": "WanderlyApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-15": "npx react-native run-ios --simulator=\"iPhone 15 Pro Max\"",
    "clean-cache": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "clean": "npx react-native-clean-project",
    "clean-files": "rm -rf yarn.lock && rm -rf Gemfile.lock && rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/Gemfile.lock  && rm -rf ios/Podfile.lock && rm -rf ios/build && rm -rf ios/.xcode.env.local && rm -rf android/Gemfile.lock",
    "setup": "yarn; yarn gem:install; yarn pod:install; yarn gradle;",
    "gem:install": "bundle install; cd ios; bundle install; cd ..; cd android; bundle install; cd ..;",
    "pod:install": "cd ios; pod install; cd ..;",
    "gradle": "cd android; ./gradlew clean; cd .."
  },
